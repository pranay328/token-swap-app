{
  "name": "@funkit/api-base",
  "version": "1.9.7",
  "description": "Base API for Funkit",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@lifeomic/attempt": "^3.1.0",
    "big.js": "^6.2.1",
    "@funkit/utils": "1.1.5"
  },
  "peerDependencies": {
    "viem": "2.x"
  },
  "devDependencies": {
    "@types/big.js": "^6.2.2",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "author": "https://fun.xyz/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fun-xyz/funkit.git",
    "directory": "packages/api-base"
  },
  "files": [
    "dist/src",
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rm -rf dist && pnpm typecheck && pnpm declaration && node build.js",
    "build:watch": "rm -rf dist && node build.js --watch",
    "typegen": "tsc --emitDeclarationOnly || true",
    "typegen:watch": "tsc --emitDeclarationOnly --watch",
    "dev": "pnpm build:watch & pnpm typegen:watch",
    "typecheck": "tsc --noEmit",
    "declaration": "tsc --emitDeclarationOnly",
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "test": "vitest"
  }
}